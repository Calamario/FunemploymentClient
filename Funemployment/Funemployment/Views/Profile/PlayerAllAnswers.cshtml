@model Funemployment.Models.PlayerAllAnswersViewModel

@foreach (var question in Model.BehaviorQuestions)
{
  <h2>@question.Content</h2>
  for (var i = 0; i < Model.BehaviorAnswers.Count(); i++)
  {
    if (Model.BehaviorAnswers[i].BQID == question.ID)
    {
      <p>@Model.BehaviorAnswers[i].Content</p>
    }
  }

}